<a href="http://localhost:3000">Back to search</a>

<h1>Search results for "{{query}}"</h1>
<main>
    {{#each artists}}
        <div class="block">
            <h2>{{this.name}}</h2>
            <img src="{{this.images.0.url}}" alt="Image of {{this.name}}">
            <a href="/albums/{{this.id}}">View Albums</a>
        </div>
    {{/each}}
</main>
